<p style="color: green"><%= notice %></p>

<%= render @track %>

<p>
    <i class="fa-solid fa-user"></i>
    <%= @track.user.name %>
</p>
<br>
<div>
  <%= link_to "Edit this track", edit_track_path(@track), class: "button is-warning" %> 
  <%= link_to "Back to tracks", tracks_path, class: "button is-dark" %>

</div>
<br>
  <%= button_to "Destroy this track", @track, class: "button is-danger", method: :delete %>